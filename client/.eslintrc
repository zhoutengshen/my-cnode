{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "rules": {
        "react/jsx-filename-extension": [
            0
        ],
        "indent": [
            2,
            4
        ],
        "react/jsx-indent": [
            2,
            4
        ],
        "no-console": [
            0
        ],
        "react/jsx-indent-props": [
            2,
            4
        ],
        "react/require-default-props": [
            0
        ],
        "react/forbid-prop-types": [
            0
        ],
        "react/prefer-stateless-function": [
            0
        ],
        "class-methods-use-this": [
            0
        ],
        "max-len": [
            0
        ],
        "react/no-danger":[0]
    }
}